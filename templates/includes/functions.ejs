<%
rarities = {
    0: "common",
    1: "uncommon",
    2: "rare",
    3: "super",
    4: "ultra",
    5: "legendary",
    6: "mythical",
    7: "god",
    8: "special",
    9: "promo"
};

getRarityName = function (number) {
    if (number in rarities) {
        return rarities[number];
    } else {
        return "unknown";
    }
}

_%>
